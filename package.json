{
  "name": "@vlad-yakovlev/grammy-reply-with-markdown",
  "version": "0.0.0",
  "description": "replyWithMarkdown method for grammY",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "compile": "tsc --noEmit",
    "lint": "eslint .",
    "lint:ci": "DEBUG=eslint:eslint eslint .",
    "prettier": "prettier --write '**/*.(ts|js|md|json)'",
    "prettier:ci": "prettier --check '**/*.(ts|js|md|json)'",
    "test": "vitest run --coverage --coverage.include='src/**/*.ts'",
    "test:ci": "prisma generate && vitest",
    "test:codecov": "prisma generate && npx vitest run --coverage --coverage.include='src/**/*.ts'"
  },
  "repository": "https://github.com/vlad-iakovlev/grammy-reply-with-markdown.git",
  "keywords": [
    "grammy",
    "telegram",
    "markdown",
    "md",
    "reply",
    "send",
    "message",
    "sendMessage"
  ],
  "author": "Vladislav Iakovlev",
  "license": "MIT",
  "bugs": "https://github.com/vlad-iakovlev/grammy-reply-with-markdown/issues",
  "homepage": "https://github.com/vlad-iakovlev/grammy-reply-with-markdown#readme",
  "dependencies": {
    "@vlad-yakovlev/telegram-md": "^2.1.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.18.12",
    "@types/node-fetch": "^2.6.13",
    "@vitest/coverage-v8": "^4.0.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "grammy": "^1.38.3",
    "jiti": "^2.6.1",
    "lefthook": "^1.13.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.3"
  }
}
